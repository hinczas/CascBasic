@model CascBasic.Models.ViewModels.ResultViewModel
<div class="alert alert-@Model.Class alert-dismissible fade show" role="alert">
    <strong>@Model.Header</strong> @Model.Message
    <button type="button" class="close" data-dismiss="alert" aria-label="Close">
        <span aria-hidden="true">&times;</span>
    </button>
</div>

